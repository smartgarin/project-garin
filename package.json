{
  "scripts": {
    "server": "cd server && nodemon --exec babel-node server.js",
    "client": "cd client && npm start",
    "start": "concurrently --kill-others-on-fail \"npm server\" \"npm client\""
  },
  "dependencies": {
    "concurrently": "^6.3.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "nodemon": "^2.0.14"
  }
}
